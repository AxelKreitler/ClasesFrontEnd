<h1>Bienvenido {{usuario}}</h1>

{{#if info}}
    <table class="table table-dark">
<thead>
    <th>ID</th>
    <th>Creador</th>
    <th>Publicacion</th>
    <th>Estado</th>
</thead>
<tbody>
{{#each info}}
    <th>{{id_publicacion}}</th>
    <th>{{nombre_usuario}}</th>
    <th>{{publicaciones}}</th>
    <th>{{estado}}</th>
    </tbody>
{{/each}}

</table>
{{else}}
<h1>No hay resultados</h1>
{{/if}}

<a href="/panel">Volver</a>